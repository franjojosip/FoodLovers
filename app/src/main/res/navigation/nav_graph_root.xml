<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_root"
    app:startDestination="@+id/nav_graph_welcome">

    <include app:graph="@navigation/nav_graph_welcome" />

    <action
        android:id="@+id/action_nav_graph_main_to_nav_graph_welcome"
        app:destination="@id/nav_graph_welcome"
        app:enterAnim="@anim/fade_in"
        app:exitAnim="@anim/fade_out"
        app:popUpTo="@id/nav_graph_welcome"
        app:popUpToInclusive="true" />

</navigation>